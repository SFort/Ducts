{
  "multipart": [
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "ducts:block/duct"
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "ducts:block/duct",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "ducts:block/duct",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "ducts:block/duct",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "down"
      },
      "apply": {
        "model": "ducts:block/duct",
        "x": 90
      }
    },
    {
      "when": {
        "facing": "up"
      },
      "apply": {
        "model": "ducts:block/duct",
        "x": -90
      }
    }
  ]
}
